      <part-detail title="f_compta.detail.compte.title_" class="2ea329d6-d3bf-4866-8979-611dbf7a8706" name="ComptaCompte">
        <keyproperties>
          <keyproperty name="CM_Numero"/>
          <keyproperty name="CM_NumCompte"/>
          <keyproperty name="CM_Nom"/>
        </keyproperties>


        <plan-search>

          <columns target-part="ComptaCompte">
            <column label="compta.compte.cm_numcompte_" value="CM_NumCompte" flex="2"/>
            <column label="compta.compte.cm_nom_"       value="CM_Nom" flex="5"/>
            <column label="compta.compte.cm_debit_"     value="CM_Debit"/>
            <column label="compta.compte.cm_credit_"    value="CM_Credit"/>
          </columns>

          <search-steps>
            <search-step orderby="1">
              <where>
                <comparison value="CM_NumCompte" is="starting-with" target="$1" />
              </where>
	    </search-step>

            <search-step orderby="1">
              <where>
                <comparison value="CM_NumCompte" is="equals-to" target="$1"/>
              </where>
	    </search-step>

            <search-step orderby="1">
              <where>
                <comparison value="CM_NumCompte" is="containing" target="$1"/>
              </where>
	    </search-step>

            <search-step orderby="2">
              <where>
                <comparison value="LOWER(CM_Nom)" is="containing" target="LOWER($1)" />
              </where>
	    </search-step>
          </search-steps>
        </plan-search>

        <plan-select>   
          <detail-block label="_.general_" orient="vertical">
            <box1 orient="vertical">
              <box2 orient="horizontal">
                <databox class="detail-title" flex="7" value="CM_Nom" align="start"/>
                <databox class="detail-title" flex="1" value="CM_NumCompte" align="end"/>
              </box2>
            </box1>
	    <box1 orient="horizontal">
              <box2 orient="vertical" flex="2">
                <databox mode="ro" label="f_compta.detail.compte.debit_"  value="CM_Debit"  flex="1" align="end"/>
                <databox mode="ro" label="f_compta.detail.compte.credit_" value="CM_Credit" flex="1" align="end"/>
                <databox mode="ro" label="f_compta.detail.compte.solde_"  value="CM_Credit-CM_Debit" align="end"/>
                <databox mode="ro" label="f_compta.detail.compte.nbecritures_" value="CM_NbEcritures" align="start"/>
              </box2>
              <box2 orient="vertical">
                <databox label="f_compta.detail.compte.estdebit_"   type="checkbox" value="CM_EstDebit" align="end"/>
                <databox label="f_compta.detail.compte.general_"    type="checkbox" value="CM_General" align="end"/>
                <databox label="f_compta.detail.compte.acceptaux_"  type="checkbox" value="CM_Acceptaux" align="end"/>
                <databox label="f_compta.detail.compte.utilisable_" type="checkbox" value="CM_Utilisable" align="start"/>
              </box2>
              <box2 orient="vertical">
                <databox label="f_compta.detail.compte.lettrable_"  type="checkbox" value="CM_Lettrable" align="start"/>
                <databox label="f_compta.detail.compte.pointable_"  type="checkbox" value="CM_Pointable" align="start"/>
                <databox label="f_compta.detail.compte.groupable_"  type="checkbox" value="CM_Groupable" align="start"/>
              </box2>
            </box1>
          </detail-block>


          <detail-block label="f_compta.detail.compte.block1_">
            <box1>
              <box2>
                <databox label="f_compta.detail.compte.acces_" value="AC_Nom" flex="5" align="start"/>
              </box2>
            </box1>
            <from>
              <join type="left-join">
                <from-table name="compta_compte"/>
                <from-table name="compta_acces"/>
                <using>
                  <column-name name="AC_Numero"/>
                </using>
              </join>
            </from>
          </detail-block>


          <detail-block label="f_compta.detail.compte.parent_" orient="horizontal">
            <box1 orient="vertical" flex="3">
              <box2>
                <databox label="f_compta.detail.compte.parent.nom_" prefix="parent" value="CM_Nom" flex="5" align="start"/>
              </box2>
              <box2 orient="horizontal">
                <databox mode="ro" label="f_compta.detail.compte.debit_" prefix="parent"  value="CM_Debit"  flex="1" align="end"/>
                <databox mode="ro" label="f_compta.detail.compte.credit_" prefix="parent" value="CM_Credit" flex="1" align="end"/>
                <databox mode="ro" label="f_compta.detail.compte.solde_"  value="parent.CM_Credit-parent.CM_Debit" align="end"/>
              </box2>
            </box1>
            <box1 orient="vertical">
              <box2 orient="vertical">
                <databox label="f_compta.detail.compte.parent.numcompte_" prefix="parent" value="CM_NumCompte" align="end"/>
                <databox mode="ro" label="f_compta.detail.compte.nbecritures_" prefix="parent" value="CM_NbEcritures" align="start"/>
              </box2>            
	    </box1>
            <from>
              <join type="left-join">
                <from-table name="compta_compte">
                  <as-table name="fils"/>
                </from-table>
                <from-table name="compta_compte">
                  <as-table name="parent"/>
                </from-table>
                <on>
                  <comparison value="fils.CM_Parent" is="equals-to" target="parent.CM_Numero"/>
                </on>
              </join>
            </from>
            <where>
              <comparison value="fils.CM_Numero" is="equals-to" target="$1"/>
            </where>
          </detail-block>
    
          <list label="f_compta.detail.compte.auxiliaires_" target-part="ComptaCompte" target-property="CM_Parent">
            <columns target-part="ComptaCompte" local-key="fils.CM_Numero">
              <column label="f_compta.detail.compte.auxiliaires.cm_numcompte_" prefix="fils" value="CM_NumCompte"/>
              <column label="f_compta.detail.compte.acces_" prefix="compta_acces" value="AC_Nom"/>
              <column label="f_compta.detail.compte.auxiliaires.cm_nom_" prefix="fils" value="CM_Nom" flex="6"/>
              <column label="compta.compte.cm_debit_" prefix="fils" value="CM_Debit"/>
              <column label="compta.compte.cm_credit_" prefix="fils" value="CM_Credit"/>
            </columns>
            <from>
              <join type="join">
                <join type="join">
                  <from-table name="compta_compte">
                    <as-table name="principal"/>
                  </from-table>
                  <from-table name="compta_compte">
                    <as-table name="fils"/>
                  </from-table>
                  <on>
                    <comparison value="fils.CM_Parent" is="equals-to" target="principal.CM_Numero"/>
                  </on>
                </join>
                <from-table name="compta_acces"/>
                <on>
                  <comparison value="fils.AC_Numero" is="equals-to" target="compta_acces.AC_Numero"/>
                </on>
              </join>
            </from>
            <where>
              <comparison value="$1" is="equals-to" target="principal.CM_Numero"/>
            </where>
          </list>
    
          <list label="f_compta.detail.compte.ecritures_">
            <columns>
              <column label="compta.piece.pi_numpiece_"    value="PI_NumPiece"/>
              <column label="compta.piece.pi_date_"        value="PI_Date" flex="2"/>
              <column label="compta.ecriture.ec_libelle_"  value="EC_Libelle" flex="5"/>
              <column label="compta.ecriture.ec_debit_"    value="EC_Debit"/>
              <column label="compta.ecriture.ec_credit_"   value="EC_Credit"/>
              <column label="compta.ecriture.ec_lettrage_" value="EC_Lettrage"/>
              <column label="compta.pointage.pt_releve_"   value="PT_Releve" flex="2"/>
            </columns>
            <from>
              <join type="join">
                <join type="join">
                  <from-table name="compta_ecriture"/>
                  <from-table name="compta_pointage"/>
                  <using>
                    <column-name name="PT_Numero"/>
                  </using>
                </join>
                <from-table name="compta_piece"/>
                <using>
                  <column-name name="PI_Numero"/>
                </using>
              </join>
            </from>
            <where>
              <comparison value="CM_Numero" is="equals-to" target="$1" />
            </where>
          </list>
        </plan-select>

	<plan-update>
          <detail-block label="_.general_" orient="vertical">
            <box1 orient="vertical">
              <box2 orient="horizontal">
                <databox label="f_compta.detail.compte.parent.nom_" flex="7" value="CM_Nom" align="start"/>
                <databox label="f_compta.detail.compte.parent.numcompte_" flex="1" value="CM_NumCompte" align="end"/>
              </box2>
              <box2 orient="horizontal">
                <databox label="f_compta.detail.compte.parent_"  type="menulist" value="CM_Parent" foreign-value="CM_Nom||' ('||CM_NumCompte||')'" align="start" foreign-key="CM_Numero" foreign-table="compta_compte"/>
              </box2>
              <box2 orient="horizontal">
                <databox label="f_compta.detail.compte.parent_"  type="menulist" value="AC_Numero" foreign-value="AC_Nom||' ('||AC_Valeur||')'" align="start" foreign-key="AC_Numero" foreign-table="compta_acces"/>
                <databox label="f_compta.detail.compte.parent_"  type="menulist" value="EX_Numero" foreign-value="EX_DateDebut||' - '||EX_DateFin" align="start" foreign-key="EX_Numero" foreign-table="compta_exercice"/>
              </box2>
            </box1>
            <box1 orient="horizontal">
              <box2 orient="vertical">
                <databox label="f_compta.detail.compte.estdebit_"   type="checkbox" value="CM_EstDebit" align="end"/>
                <databox label="f_compta.detail.compte.general_"    type="checkbox" value="CM_General" align="end"/>
                <databox label="f_compta.detail.compte.acceptaux_"  type="checkbox" value="CM_Acceptaux" align="end"/>
                <databox label="f_compta.detail.compte.utilisable_" type="checkbox" value="CM_Utilisable" align="start"/>
              </box2>
              <box2 orient="vertical">
                <databox label="f_compta.detail.compte.lettrable_"  type="checkbox" value="CM_Lettrable" align="start"/>
